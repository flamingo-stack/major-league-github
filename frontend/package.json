{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "npx webpack serve --mode development",
    "prebuild": "npm run setup:ui-kit",
    "build": "./node_modules/.bin/webpack --mode production",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "generate-favicon": "node scripts/generate-favicon.js",
    "setup:ui-kit": "if [ ! -d \"ui-kit\" ]; then git clone https://github.com/flamingo-stack/ui-kit.git ui-kit && cd ui-kit && npm install; elif [ ! -d \"ui-kit/node_modules\" ]; then cd ui-kit && npm install; else echo 'ui-kit already set up'; fi",
    "copy:colors": "node scripts/extract-ui-kit-colors.js"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@flamingo/ui-kit": "file:./ui-kit",
    "@mui/icons-material": "^6.3.1",
    "@mui/material": "^6.3.1",
    "@react-pdf/renderer": "^4.1.6",
    "@tanstack/query-core": "5.64.0",
    "@types/lodash": "^4.17.14",
    "@types/react-query": "1.2.9",
    "axios": "^1.8.2",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.2",
    "lodash": "^4.17.21",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@eslint/js": "^9.17.0",
    "@svgr/webpack": "^8.1.0",
    "@tanstack/react-query": "5.64.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "babel-loader": "^9.2.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.9.1",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "file-loader": "^6.2.0",
    "globals": "^15.14.0",
    "html-webpack-plugin": "^5.6.0",
    "sharp": "^0.34.3",
    "style-loader": "^3.3.4",
    "to-ico": "^1.1.5",
    "ts-loader": "^9.5.1",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "url-loader": "^4.1.1",
    "vite-plugin-svgr": "^4.3.0",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.1"
  }
}
